    "version": "0.2.0",
    "configurations": [
    {
        "name": "FastAPI Debug",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
            "src.main:app",
            "--reload"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
"justMyCode": true    },
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": true
        }
    ]
}
